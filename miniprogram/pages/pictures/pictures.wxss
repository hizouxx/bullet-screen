/* miniprogram/pages/picture/pictrue.wxss */
.container{
  padding-top: 80rpx;
  position: relative;
}
.upload{
  position: absolute;
  right: 30rpx;
  top: 10rpx;
}
.list{
  padding: 20rpx 0;
}

.list-item{
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: space-between
}
.list-item-datetime{
  width: 200rpx;
  font-size: 24rpx;
}
.list-item-imgs{
  width: 500rpx;
  display: flex;
  flex-wrap: wrap;
}
.list-item-imgs .img{
  height: 154rpx;
  width: 154rpx;
  margin: 10rpx 10rpx 0 0;
  border: 1rpx solid #f1f1f1;
}
